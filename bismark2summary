	<script type='text/javascript'>
    	// From http://stackoverflow.com/questions/2901102/how-to-print-a-number-with-commas-as-thousands-separators-in-javascript
    function numberWithCommas(x) {
            try {
                var withcommas = x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
            } catch(e){
                var withcommas = x;
            }
            return x;
        }

    	// From http://stackoverflow.com/questions/18485939/highcharts-change-column-stacking-on-click/18486302
    	$(function () {
    		$('.switch_group button').click(function(e){
    			e.preventDefault();
				$(this).siblings('button.active').removeClass('active');
				$(this).addClass('active');
                var target = $(this).data('target');
				var action = $(this).data('action');
				// Counts / Percentages
    			if(action == 'set_percent' || action == 'set_numbers'){
					var sym = (action == 'set_percent') ? '%' : '#';
					var stack = (action == 'set_percent') ? 'percent' : 'normal';
					if(target == 'methylation_context'){
						meth_plot_options.yAxis.title.text = sym+' Calls';
						meth_plot_options.plotOptions.area.stacking = stack;
						$('#methylation_context').highcharts(meth_plot_options);
					} else if(target == 'alignment'){
						alignment_plot_options.yAxis.title.text = sym+' Reads';
						alignment_plot_options.plotOptions.area.stacking = stack;
						$('#alignment').highcharts(alignment_plot_options);
					}
    			}
				// CpG / CHG / CHH
				else if (action == 'set_meth_type'){
					var m_type = $(this).text().trim();
					meth_plot_options.series = meth_calls_data[m_type];
					if(m_type == 'CpG'){
						meth_plot_options.colors = meth_plot_colors.slice(0, 2);
					} else if(m_type == 'CHG'){
						meth_plot_options.colors = meth_plot_colors.slice(2, 5);
					} else if(m_type == 'CHH'){
						meth_plot_options.colors = meth_plot_colors.slice(4, 7);
					}
					$('#methylation_context').highcharts(meth_plot_options);
				}
    		});
		    // Plot setup
			var alignment_plot_colors = ['#01665e','#f28f43', '#0d233a', '#492970', '#2f7ed8', '#8bbc21'];
			var meth_plot_colors = ['#0d233a', '#2f7ed8', '#8bbc21', '#1aadce', '#910000', '#492970'];
			var num_samples = {{num_samples}};
			// Alignment Plot
			var aln_x_index = num_samples * -1;
			var alignment_plot_options = {
				colors: alignment_plot_colors,
				chart: {
					type: 'area',
					events: {
						load: function(){
							aln_x_index = num_samples * -1;
						},
						redraw: function(){
							aln_x_index = num_samples * -1;
						}
					}
				},
				title: {
					text: ''
				},
				xAxis: {
					title: {
						text: 'Sample',
						margin: 15,
					},
					tickmarkPlacement: 'on',
					labels: {
						formatter: function() {
							aln_x_index++;
							return aln_x_index;
						}
					},
					categories: [ {{categories}} ]
				},
				credits: {
			        text: 'Hover to identify samples and numbers',
			        position: {
			            align: 'center',
						x: 30
			        },
					href: null,
					style: {
			            cursor: 'default'
			        }
				},
				yAxis: {
					min: 0,
					title: {
						text: '# Reads'
					}
				},
				legend: {
					align: 'right',
					verticalAlign: 'top',
					y: 10,
					floating: true,
					backgroundColor: (Highcharts.theme && Highcharts.theme.legendBackgroundColorSolid) || 'white',
					borderColor: '#CCC',
					borderWidth: 1,
					shadow: false
				},
				tooltip: {
                    formatter: function () {
                        var s = '<table><tr><th colspan="3" style="font-weight:bold; text-decoration:underline;">' + this.x + '</th></tr>';
                        $.each(this.points, function () {
                            if(this.y !== null){
                                s += '<tr><td style="font-weight:bold; color:'+this.series.color+';">' + this.series.name + ':</td><td style="text-align:right;">' + numberWithCommas(this.y) + ' reads</td><td style="text-align:right;">(' + this.percentage.toFixed(1) + '%)</td></tr>';
                            }
                        });
                        s += '</table>';
                        return s;
                    },
                    shared: true,
                    useHTML: true
                },	    
				plotOptions: {
					area: {
						stacking: 'normal',
						lineColor: '#666666',
						lineWidth: 1,
						marker: {
						    enabled: false
						}
					}
				},
				series: [
				    {
				      name: 'Unique Alignments',
				      data: [ {{aligned_seq}} ]
				    },
				    {
				      name: 'No Genomic Sequence',
				      data: [ {{no_seq}} ]
				    },
				    {
				      name: 'Did Not Align',
				      data: [ {{not_aligned}} ]
				    },
				    {
				      name: 'Aligned Ambiguously',
				      data: [ {{ambig_aligned}} ]
				    },
				    {
				      name: 'Duplicate Alignments',
				      data: [ {{dup_alignments}} ]
				    },
				    {
				      name: 'Deduplicated Unique Alignments',
				      data: [ {{unique_alignments}} ]
				    }
				  ]
			}
			$('#alignment').highcharts(alignment_plot_options);
		    // Methylated Calls Plot
			var meth_calls_data = {
				CpG: [{
					name: 'Unmethylated CpG',
					data: [ {{unmeth_cpg_string}} ]
				},
	            {
	                name: 'Methlyated CpG',
	                data: [ {{meth_cpg_string}} ]
	            }],
				CHG: [{
					name: 'Unmethylated CHG',
					data:  [ {{unmeth_cph_string}} ]
				},
				{
		            name: 'Methlyated CHG',
		            data:  [ {{meth_cph_string}} ]
		        }],
				CHH: [{
					name: 'Unmethylated CHH',
					data:  [ {{unmeth_chh_string}} ]
				},
				{
		            name: 'Methlyated CHH',
		            data:  [ {{meth_chh_string}} ]
		        }]
			};
			var meth_x_index = num_samples * -1;
			var meth_plot_options = {
				colors: meth_plot_colors,
				chart: {
					type: 'area',
					events: {
						load: function(){
							meth_x_index = num_samples * -1;
						},
						redraw: function(){
							meth_x_index = num_samples * -1;
						}
					}
				},
				title: {
					text: ''
				},
				xAxis: {
					title: {
						text: 'Sample',
						margin: 15,
					},
					tickmarkPlacement: 'on',
					labels: {
						formatter: function() {
							meth_x_index++;
							return meth_x_index;
						}
					},
					categories: [ {{categories}} ]
				},
				credits: {
			        text: 'Hover to identify samples and numbers',
			        position: {
			            align: 'center',
						x: 30
			        },
					href: null,
					style: {
			            cursor: 'default'
			        }
				},
				yAxis: {
					min: 0,
					title: {
						text: '% Calls'
					},
					stackLabels: {
						enabled: false
					}
				},
				legend: {
					align: 'right',
					verticalAlign: 'top',
					y: 10,
					floating: true,
					backgroundColor: (Highcharts.theme && Highcharts.theme.legendBackgroundColorSolid) || 'white',
					borderColor: '#CCC',
					borderWidth: 1,
					shadow: false
				},
				tooltip: {
					formatter: function () {
		                var s = '<table><tr><th colspan="3" style="font-weight:bold; text-decoration:underline;">' + this.x + '</th></tr>';
		                $.each(this.points, function () {
		                    s += '<tr><td style="font-weight:bold; color:'+this.series.color+';">' + this.series.name + ':</td><td style="text-align:right;">' + numberWithCommas(this.y) + ' calls</td><td style="text-align:right;">(' + this.percentage.toFixed(1) + '%)</td></tr>';
		                });
						s += '</table>';
		                return s;
		            },
					shared: true,
					useHTML: true
				},
				plotOptions: {
					area: {
						stacking: 'percent',
						lineColor: '#666666',
						lineWidth: 1,
						marker: {
						    enabled: false
						}
					}
				},
				series: meth_calls_data['CpG']
			};
			$('#methylation_context').highcharts(meth_plot_options);
		});
	</script>
<div class="container">
	<div class="header">
		<img alt="Bismark" src="data:image/png;base64," />
		<h1>Bismark Project Overall Summary</h1>
		<div class="subtitle">
			<h3>{{page_title}}</h3>
			<p>Report generated at {{report_timestamp}}</p>
		</div>
	</div>
	<hr>
	<h2>
		Alignment
	    <div class="switch_group">
			<button class="active" data-action="set_numbers" data-target="alignment">Number of Reads
			</button><button data-action="set_percent" data-target="alignment">Percentages</button>
		</div>
	</h2>
	<div id="alignment" class="plot"></div>
	<hr>
	<h2>
		Cytosine Methylation
		<div class="switch_group">
			<button data-action="set_numbers" data-target="methylation_context">Number of Calls
			</button><button class="active" data-action="set_percent" data-target="methylation_context">Percentages</button>
		</div>
		<div class="switch_group">
			<button class="active" data-action="set_meth_type" data-target="methylation_context">CpG
			</button><button data-action="set_meth_type" data-target="methylation_context">CHG
			</button><button data-action="set_meth_type" data-target="methylation_context">CHH</button>
		</div>
	</h2>
